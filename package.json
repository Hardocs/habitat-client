{
  "name": "@hardocs-project/habitat-client",
  "version": "0.5.32",
  "description": "Habitat Client API",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "yarn prepublish",
    "prepublish": "babel src -d lib",
    "drypub": "npm publish --access public --dry-run",
    "vpub": "npm publish --registry http://localhost:4873",
    "ppub": "npm publish --access public",
    "apub": "npm publish --access public --tag alpha",
    "bpub": "npm publish --access public --tag beta",
    "rpub": "npm publish --access public --tag rc",
    "npub": "npm publish --access public --tag next",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Narration SD",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "pouchdb": "^7.2.2",
    "pouchdb-fetch": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-upsert": "^2.2.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
